<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="xuqiu.spring.Mapper.HomeInfopMapper">

    <update id="edit" parameterType="HomeInfo" >
        update homelist set fzr=#{fzr},kcjj=#{kcjj},kcts=#{kcts},kcdt=#{kcdt},jxtd=#{jxtd}
    </update>

    <select id="query" resultType="HomeInfo" >
        select * from homelist
    </select>

</mapper>